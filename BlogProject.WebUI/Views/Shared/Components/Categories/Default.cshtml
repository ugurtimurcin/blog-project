@model List<CategoryListDto>

<div class="card my-4">
    <h5 class="card-header">Categories</h5>
    <div class="list-group">
        @foreach (var category in Model)
        {
            <a href="/" class="list-group-item d-flex justify-content-between align-items-center list-group-item-action border-top-0 border-left-0 border-right-0">
                @category.Name
                <span class="badge badge-primary badge-pill">14</span>
            </a>
        }
    </div>
</div>